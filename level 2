    <script>
        var arr = [[1, 2], [3, 4,], [6, 7, 9, [11, 12, [12, 13, [14]]]], 10];
        function even(arr) {
            for (var i = 0; i < arr.length - 1; i++) 
            {
                for (j = 0; j<arr.length - i - 1; j++) 
                {
                    if (arr[j] > arr[j + 1]) 
                    {
                        var temp = arr[j + 1];s
                        arr[j + 1] = arr[j];
                        arr[j] = temp;
                    }
                }
            }
            return arr;
        }
        even(arr);
        console.log(arr);
    </script>
