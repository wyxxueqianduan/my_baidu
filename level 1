    <Script>
        var arr = ['ABCDEEF,GHJKKM'];
        var brr = [];
        function recur(arr) {
            for (var i = 0; i < arr.length; i++) 
            {
                brr = arr[i].split("");
                brr= brr.join("");
                brr = brr.match(/(\w)\1+/g);
                console.log(brr);
            }
        }
        recur(arr);
    </Script>
