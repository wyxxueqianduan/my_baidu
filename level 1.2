<body>
    <script>
        var arr = [1,2,3,5,4,6,5,5,5];
        function count(arr,n){ 
          var count = 0;
          for(var i = 0;i<arr.length;i++)
          {
                if(arr[i] == n)
                {
                    count++;
                }
          }
          console.log(count);
        }
        count(arr,5);
    </script>
